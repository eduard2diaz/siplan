<div class="modal-dialog modal-full">
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title">Detalles del objetivo</h4>
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
        </div>
        <div>
            <table class="table" style="margin-bottom: 0px">
                <tr>
                    <th style="width: 5%">Número</th>
                    <td>{{ objetivo.numero }}</td>
                </tr>
                <tr>
                    <th>Descripción</th>
                    <td><p>{{ objetivo.descripcion}}</p></td>
                </tr>
            </table>
        </div>
        <div class="modal-footer">
            <a class="btn btn-sm btn-metal active" data-dismiss="modal"><i class="flaticon-cancel"></i>Cancelar</a>
        </div>
    </div>
</div>